{
  "name": "pbtson",
  "version": "1.0.0",
  "description": "Protobuf to TypeScript .d.ts for JSON objects generator",
  "main": "index.js",
  "license": "MIT",
  "bin": {
    "protoc-gen-pbtson": "./bin/protoc-gen-pbtson"
  },
  "dependencies": {
    "google-protobuf": "^3.6.1",
    "stream-to-promise": "^2.2.0"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.2.7",
    "@types/node": "^10.12.18",
    "@types/stream-to-promise": "^2.2.0",
    "tslint": "^5.12.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.2.2"
  },
  "scripts": {
    "build": "tsc -p ./src/tsconfig.json",
    "lint": "tslint \"./src/**/*.ts\"",
    "typecheck": "tsc --noEmit -p ./src/tsconfig.json"
  }
}
